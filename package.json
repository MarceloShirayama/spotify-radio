{
  "name": "spotify-radio",
  "version": "1.0.0",
  "description": "Seja bem vindo(a) à sexta Semana Javascript Expert. Este é o código inicial para iniciar nossa jornada.",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "live:reload": "nodemon server/index.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --color --no-cache",
    "test:watch": "LOG_DISABLED=true npm run test -- --watchAll",
    "test:coverage": "LOG_DISABLED=true npm run test -- --coverage tests/unit",
    "test:e2e": "LOG_DISABLED=true npm run test -- tests/e2e",
    "live-reload:docker": "docker-compose up --build live-reload",
    "test:docker": "docker-compose up --build test",
    "test-watch:docker": "docker-compose up --build test-watch",
    "test-coverage:docker": "docker-compose up --build test-coverage",
    "docker:bash": "docker exec -it spotify-radio bash"
  },
  "engines": {
    "node": "17"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pino": "7.9.2",
    "pino-pretty": "7.6.0",
    "throttle": "1.0.3"
  },
  "devDependencies": {
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "portfinder": "1.0.28",
    "supertest": "6.2.2"
  }
}
